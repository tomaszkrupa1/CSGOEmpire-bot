{
  "name": "EAB",
  "description": "CSGO Empire Auto Better Bot",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Tomasz Krupa",
  "icons": {
    "32": "/icons/32.png",
    "192": "/icons/192.png"
  },
  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["https://www.csgoempire.com/*"],
      "js": ["script.js", "menu.js"]
    }
  ],
  "browser_action": {
    "default_popup": "menu.html"
  }
}
